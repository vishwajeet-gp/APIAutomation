import java.util.ArrayList;
import java.util.Collections;
import java.util.List;

public class AscendingOrder {

	public static void main(String[] args)
	{
	    ArrayList al=new ArrayList();
		  al.add(10);
		  al.add(16);
		  al.add(101);
		  al.add(130);
		  al.add(20);
	    
	    Collections.sort(al);
	}

}
